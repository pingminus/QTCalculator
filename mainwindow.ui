        <string notr="true">/* Allgemeiner Stil für alle Buttons */
QPushButton {
    background-color: #f1f2f6;
    color: #2f3542;
    border: none;
    border-radius: 25px;
    font-size: 20px;
    font-weight: bold;
    padding: 10px;
}

/* Hover-Effekt für alle Buttons */
QPushButton:hover {
    background-color: #dfe4ea;
}

/* Pressed-Effekt für alle Buttons */
QPushButton:pressed {
    background-color: #ced6e0;
}

/* Spezieller Stil für den Clear-Button */
QPushButton#pushButtonClear {
    background-color: #ff7f50; /* Helles Orange */
    color: #fff;
}

/* Hover-Effekt für den Clear-Button */
QPushButton#pushButtonClear:hover {
    background-color: #ff6347; /* Dunkleres Orange bei Hover */
}

/* Pressed-Effekt für den Clear-Button */
QPushButton#pushButtonClear:pressed {
    background-color: #e55347; /* Noch dunkleres Orange bei gedrückt */
}
